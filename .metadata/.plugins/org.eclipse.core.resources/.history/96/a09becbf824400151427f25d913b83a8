package fasta2tbl;

public class Fasta2TblApp {
	public static void main(String[] args) {
		String filename;
		if (args.length == 1) {
			try {
				filename = args[0];
			} catch {
				
			}
		} else {
			System.out.println("Input only one argument for the fasta file!");
			System.exit(0);
		}
		
	} // end main

}
